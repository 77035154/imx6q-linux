    "periph_pre", "periph_clk2", "periph_clk2_sel", "osc";
   interrupts = <0 107 0x04>, <0 112 0x4>, <0 113 0x4>, <0 114 0x4>;
   interrupt-names = "irq_busfreq_0", "irq_busfreq_1", "irq_busfreq_2", "irq_busfreq_3";
   fsl,max_ddr_freq = <528000000>;
  };

                gpu@00130000 {
