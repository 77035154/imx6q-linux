/ {
 aliases {
  ipu1 = &ipu2;
  spi4 = &ecspi5;
 };

 cpus {
  #address-cells = <1>;
  #size-cells = <0>;

  cpu0: cpu@0 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <0>;
   next-level-cache = <&L2>;
   operating-points = <

    1200000 1275000
    996000 1250000
    852000 1250000
    792000 1175000
    396000 975000
   >;
   fsl,soc-operating-points = <

    1200000 1275000
    996000 1250000
    852000 1250000
    792000 1175000
    396000 1175000
   >;
   clock-latency = <61036>;
   clocks = <&clks 104>,
     <&clks 6>,
     <&clks 16>,
     <&clks 17>,
     <&clks 170>,
     <&clks 231>,
     <&clks 238>,
     <&clks 224>,
     <&clks 99>;
   clock-names = "arm", "pll2_pfd2_396m", "step",
          "pll1_sw", "pll1_sys", "pll1",
          "pll1_bypass", "pll1_bypass_src",
          "vpu_axi_podf";
   arm-supply = <&reg_arm>;
   pu-supply = <&reg_pu>;
   soc-supply = <&reg_soc>;
  };

  cpu@1 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <1>;
   next-level-cache = <&L2>;
  };

  cpu@2 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <2>;
   next-level-cache = <&L2>;
  };

  cpu@3 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <3>;
   next-level-cache = <&L2>;
  };
 };

 reserved-memory {
  #address-cells = <1>;
  #size-cells = <1>;
  ranges;


  linux,cma {
   compatible = "shared-dma-pool";
   reusable;
   size = <0x14000000>;
   linux,cma-default;
  };
 };

