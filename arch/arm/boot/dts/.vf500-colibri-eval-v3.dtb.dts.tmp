    clock-names = "ipg";
    dmas = <&edma0 0 50>,
     <&edma0 0 51>;
    dma-names = "rx","tx";
    status = "disabled";
   };

   clks: ccm@4006b000 {
    compatible = "fsl,vf610-ccm";
    reg = <0x4006b000 0x1000>;
