/ {
 aliases {
  can0 = &can0;
  can1 = &can1;
  serial0 = &uart0;
  serial1 = &uart1;
  serial2 = &uart2;
  serial3 = &uart3;
  serial4 = &uart4;
  serial5 = &uart5;
  gpio0 = &gpio0;
