    status = "disabled";

    port {
     tve_in: endpoint {
      remote-endpoint = <&ipu_di1_tve>;
     };
    };
   };

   vpu: vpu@63ff4000 {
    compatible = "fsl,imx53-vpu", "cnm,coda7541";
    reg = <0x63ff4000 0x1000>;
    interrupts = <9>;
    clocks = <&clks 64>,
             <&clks 63>;
    clock-names = "per", "ahb";
    resets = <&src 1>;
    iram = <&ocram>;
   };

   sahara: crypto@63ff8000 {
    compatible = "fsl,imx53-sahara";
    reg = <0x63ff8000 0x4000>;
    interrupts = <19 20>;
    clocks = <&clks 187>,
             <&clks 187>;
    clock-names = "ipg", "ahb";
   };
  };

  ocram: sram@f8000000 {
   compatible = "mmio-sram";
   reg = <0xf8000000 0x20000>;
   clocks = <&clks 186>;
  };

  pmu {
   compatible = "arm,cortex-a8-pmu";
   interrupts = <77>;
  };
 };
};
